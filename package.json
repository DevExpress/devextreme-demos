    "scripts": {
        "test": "jest --coverage --runInBand",
        "postinstall": "npm run prepare-js",
        "clean": "git clean -f -x -d",
        "add-demo": "node utils/interactive/add-demo.js",
        "link-repos": "node utils/interactive/link-repos.js",
        "create-config": "node utils/internal/create-config.js",
        "launch-demo": "node utils/shell/server.js",
        "prepare-ts": "node ./node_modules/typescript/bin/tsc -p JSDemos/shared",
        "prepare-js": "npm run prepare-ts && gulp js",
        "prepare-aspnet": "gulp aspnet",
        "prepare-all": "npm run prepare-js && npm run prepare-aspnet",
        "prepare-shared": "npm run prepare-ts && gulp shared",
        "lint-html": "prettier --check .",
        "lint-js": "eslint .",
        "lint-css": "stylelint **/*.{css,vue}",
        "lint-mvc": "cd MVCDemos && eclint check **/*.cshtml !{bin,obj}/** && dotnet format -w DevExtreme.MVC.Demos.sln --check && cd ../",
        "lint-netcore": "cd NetCoreDemos && eclint check **/*.cshtml !{bin,obj}/** !DemoShell/** && dotnet format -w DevExtreme.NETCore.Demos.sln --check && cd ../",
        "lint": "npm-run-all -p -c lint-js lint-mvc lint-netcore lint-css lint-html",
        "webserver": "http-server --port 8080 -c-1",
        "test-testcafe": "node utils/visual-tests/testcafe-runner.mjs",
        "test-testcafe:accessibility": "cross-env STRATEGY=accessibility CONSTEL=jquery node utils/visual-tests/testcafe-runner.mjs",
        "fix-lint": "prettier --write . && eslint --fix . && stylelint **/*.{css,vue} --fix",
        "prettier": "prettier",
        "build": "node utils/bundle/index.js",
        "build-angular": "npm run build -- angular",
        "build-react": "npm run build -- react",
        "build-vue": "npm run build -- vue",
        "build-bundles": "npm-run-all -p build-angular build-react build-vue",
        "prepare-bundles": "npm run build-bundles && gulp bundles",
        "convert-to-js": "ts-node ./utils/ts-to-js-converter/cli.ts"
    },
