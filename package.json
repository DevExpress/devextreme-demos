    "scripts": {
        "add-demo": "node utils/interactive/add-demo.js",
        "link-repos": "node utils/interactive/link-repos.js",
        "create-config": "node utils/internal/create-config.js",
        "launch-demo": "node utils/interactive/launch-demo.js",
        "prepare-js": "node utils/internal/create-config.js",
        "prepare-js:dev": "node utils/create-config.js && node utils/interactive/link-repos.js",
        "prepare-mvc": "node utils/internal/prepare-mvc.js",
        "prepare-netcore": "node utils/internal/prepare-netcore.js",
        "prepare-all": "npm run prepare-js:dev && npm run prepare-mvc && npm run prepare-netcore",
        "lint-js": "eslint .",
        "lint-react": "eslint JSDemos/Demos -c .react.eslintrc.json --ignore-path .react.eslintignore",
        "lint-vue": "eslint JSDemos/Demos -c .vue.eslintrc.json --ignore-path .vue.eslintignore --ext .vue,.js",
        "lint-mvc": "cd MVCDemos && eclint check **/*.cshtml !{bin,obj}/** && dotnet format -w DevExtreme.MVC.Demos.sln --dry-run --check && cd ../",
        "lint-netcore": "cd NetCoreDemos && eclint check **/*.cshtml !{bin,obj}/** !DemoShell/** && dotnet format -w DevExtreme.NETCore.Demos.sln --dry-run --check && cd ../",
        "lint": "npm-run-all -p -c lint-js lint-react lint-vue lint-mvc lint-netcore"
    },
