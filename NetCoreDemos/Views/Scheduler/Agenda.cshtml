@model IEnumerable<DevExtreme.NETCore.Demos.Models.Appointment>

@(Html.DevExtreme().Scheduler()
    .ID("scheduler")
    .DataSource(Model)
    .TimeZone("America/Los_Angeles")
    .Views(new[] { SchedulerViewType.Agenda })
    .CurrentView(SchedulerViewType.Agenda)
    .CurrentDate(new DateTime(2021, 5, 11))
    .StartDayHour(9)
    .Height(600)
    .TextExpr("Text")
    .StartDateExpr("StartDate")
    .EndDateExpr("EndDate")
)
